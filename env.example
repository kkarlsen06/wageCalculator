# Vite Environment Variables (Frontend)
# These variables are prefixed with VITE_ and are available in the frontend build
VITE_SUPABASE_URL=https://id.kkarlsen.dev
VITE_SUPABASE_PUBLISHABLE_KEY=sb_publishable_your_publishable_key

# Node/Server/Functions Environment Variables (Backend)
# These variables are used by the backend server
SUPABASE_URL=https://id.kkarlsen.dev
SUPABASE_SERVICE_ROLE_KEY=sb_secret_your_service_role_key

# Stripe
STRIPE_SECRET_KEY=sk_live_or_test_key
STRIPE_WEBHOOK_SECRET=whsec_your_endpoint_secret

# Optional: explicitly set app base URL for checkout redirects
# If not set, server will derive from X-Forwarded-* headers
# APP_BASE_URL=https://your-frontend-domain.com

# Base URL used for Stripe portal return redirect (must be the public origin)
PUBLIC_APP_BASE_URL=https://kkarlsen.dev


OPENAI_API_KEY=sk-proj-1234567890

NODE_ENV=development/production
DEV_BYPASS=true/false
